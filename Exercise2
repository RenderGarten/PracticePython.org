user_num = int(input('Please Choose a Number to Analyze'))
user_check = int(input('Enter a number to divide by'))

if user_num%user_check==0:
    print('Your second number divides evenly into your first!')
if user_num%4==0:
    print('Wow! this number is a multiple of 4!')
elif user_num%2==0:
    print('Your number is even')
else:
    print('This number is odd')


